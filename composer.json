{
    "name": "endroid/cm-sms-bundle",
    "description": "Endroid CM SMS Bundle",
    "keywords": ["endroid", "cm", "sms", "symfony", "bundle"],
    "homepage": "https://github.com/endroid/EndroidCmSmsBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeroen van den Enden",
            "email": "info@endroid.nl",
            "homepage": "http://endroid.nl/"
        }
    ],
    "require": {
        "endroid/cm-sms": "^2.0",
        "twig/twig": "^2.4",
        "doctrine/orm": "^2.5",
        "symfony/asset": "^3.0|^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Endroid\\CmSmsBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Endroid\\CmSmsBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    }
}
