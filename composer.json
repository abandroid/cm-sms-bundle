{
    "name": "endroid/cm-sms-bundle",
    "description": "Endroid CM SMS Bundle",
    "keywords": ["endroid", "cm", "sms", "symfony", "bundle", "php"],
    "homepage": "https://github.com/endroid/cm-sms-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeroen van den Enden",
            "email": "info@endroid.nl"
        }
    ],
    "require": {
        "php": ">=7.2",
        "endroid/cm-sms": "^2.0",
        "twig/twig": "^2.4",
        "doctrine/orm": "^2.6",
        "symfony/asset": "^3.4||^4.0||^5.0",
        "symfony/web-link": "^3.4||^4.0||^5.0",
        "doctrine/doctrine-bundle": "^1.8.1"
    },
    "require-dev": {
        "endroid/test": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Endroid\\CmSmsBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Endroid\\CmSmsBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
